<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="ZSeekBar">
        <attr name="zStartAngle" format="float"/>
        <attr name="zSweepAngle" format="float"/>
        <attr name="zProgress" format="float"/>
        <attr name="zMaxProgress" format="float"/>
        <attr name="zIsRoundEdges" format="boolean"/>
        <attr name="zIsClockwise" format="boolean"/>
        <attr name="zIsFloatingThumbColor" format="boolean"/>
        <attr name="zProgressBackgroundColor" format="color"/>
        <attr name="zProgressColorList" format="string"/>
        <attr name="zProgressColor" format="color"/>
        <attr name="zProgressCenterColor" format="color"/>
        <attr name="zProgressEndColor" format="color"/>
        <attr name="zProgressWidth" format="dimension"/>
        <attr name="zProgressBackgroundWidth" format="dimension"/>
        <attr name="zThumbWidth" format="dimension"/>
        <attr name="zThumbTintColor" format="color"/>
        <attr name="zThumb" format="reference"/>

        <attr name="zSemiCircleGravity">
            <enum name="none" value="0"/>
            <enum name="top" value="1"/>
            <enum name="bottom" value="2"/>
        </attr>

        <attr name="zSeekMode">
            <enum name="circle" value="0"/>
            <enum name="arc" value="1"/>
            <enum name="line" value="2"/>
        </attr>
    </declare-styleable>

    <!---->
    <attr name="zSeekBarStyle" format="reference"/>

</resources>